{
  "id": null,
  "title": "k6 Performance Test Dashboard",
  "tags": ["k6", "performance", "lunchchat"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Virtual Users",
      "type": "stat",
      "targets": [
        {
          "expr": "k6_vus",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "red", "value": 80}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      },
      "pluginVersion": "8.0.0",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Request Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(k6_http_reqs_total[1m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "thresholds"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Response Time",
      "type": "graph",
      "targets": [
        {
          "expr": "k6_http_req_duration{quantile=\"0.95\"}",
          "refId": "A",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "k6_http_req_duration{quantile=\"0.5\"}",
          "refId": "B",
          "legendFormat": "50th percentile"
        }
      ],
      "yAxes": [
        {
          "unit": "ms"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Error Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(k6_http_reqs_total{status!~\"2..\"}[5m]) / rate(k6_http_reqs_total[5m]) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 5,
      "title": "Data Received",
      "type": "stat",
      "targets": [
        {
          "expr": "k6_data_received_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {
            "mode": "thresholds"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    }
  ],
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "refresh": "5s"
}